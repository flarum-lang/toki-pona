{
	"name": "flarum-lang/toki-pona",
	"type": "flarum-extension",
	"description": "Toki Pona language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/toki-pona",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"tok-locale",
		"toki-pona-locale",
		"toki-pona"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/toki-pona/issues",
		"source": "https://github.com/flarum-lang/toki-pona"
	},
	"require": {
		"flarum/core": "^1.7"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Toki Pona",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "tok",
